{% extends 'base.html.twig' %}

{% block title %}Hello StreakController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>



{{ dump(invoices) }}

<table class="table table-striped">
    <thead class="table-dark">
        <tr>
            <th>ID</th>
            <th>Número de Factura</th>
            <th>Comentario</th>
            <th>Status</th>
            <th>Fecha de Factura</th>
            <th>Fecha de Creación</th>
            <th>RFC</th>
            <th>Monto</th>
            <th>uuid</th>
            <th>pdf<th>
            <th>xml<th>
        </tr>
    </thead>
    <tbody>
        {% for invoice in invoices %}
            <tr>
                <td>{{ invoice.id }}</td>
                <td>{{ invoice.ninvoice }}</td>
                <td>{{ invoice.comment }}</td>
                <td>{{ invoice.priority }}</td>
                <td>{{ invoice.dateInvoice ? invoice.dateInvoice|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ invoice.createAt ? invoice.createAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ invoice.rfc }}</td>
                <td>{{ invoice.mount }}</td>
                <td>{{ invoice.response ? invoice.response.uuid : '' }}</td>
